coroutine.wrap(OCQY_fake_script)()
local function YDVJD_fake_script() -- TextButton.LocalScript 
	local script = Instance.new('LocalScript', TextButton)

	local keys = {
		LeftShift = false,
	}
	game.Players.LocalPlayer:GetMouse().KeyDown:Connect(function(key)
		if key == "0" then
			keys.LeftShift = true
		end
	end)
	game.Players.LocalPlayer:GetMouse().KeyUp:Connect(function(key)
		if key == "0" then
			keys.LeftShift = false
		end
	end)
	local hack = false
	script.Parent.MouseButton1Click:Connect(function()
		hack = not hack
		if hack then
			script.Parent.Text = "Fly: on"
			script.Parent.BackgroundColor3 = Color3.fromRGB(0,255,0)
			local humanoid = nil
			while hack do
				local fps = game["Run Service"].RenderStepped:Wait()
				if not humanoid or not humanoid.Parent or not humanoid.Parent.Parent then
					if game.Players.LocalPlayer.Character then
						if game.Players.LocalPlayer.Character:FindFirstChild("Humanoid") then
							humanoid = game.Players.LocalPlayer.Character.Humanoid
							humanoid.PlatformStand = true
						end
					end
				else
					humanoid.PlatformStand = true
					if humanoid.RootPart then
						if humanoid.RootPart:FindFirstChild("NoGravity") then
							humanoid.RootPart.NoGravity.Force = Vector3.new(0,workspace.Gravity*humanoid.RootPart.AssemblyMass,0)
						else
							local no = Instance.new("BodyForce",humanoid.RootPart)
							no.Name = "NoGravity"
							no.Force = Vector3.new(0,workspace.Gravity*humanoid.RootPart.AssemblyMass,0)
						end
						local vel = humanoid.MoveDirection*humanoid.WalkSpeed
						if humanoid.Jump then
							vel = vel+Vector3.new(0,humanoid.WalkSpeed,0)
						end
						if keys.LeftShift then
							vel = vel+Vector3.new(0,-humanoid.WalkSpeed,0)
						end
						humanoid.RootPart.Velocity = ((humanoid.RootPart.Velocity-vel)*(0.75^(30/(1/fps))))+vel
					end
				end
			end
		else
			script.Parent.Text = "Fly: off"
			script.Parent.BackgroundColor3 = Color3.fromRGB(255,0,0)
			game["Run Service"].RenderStepped:Wait()
			game["Run Service"].RenderStepped:Wait()
			if game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:FindFirstChild("Humanoid") then
				game.Players.LocalPlayer.Character.Humanoid.PlatformStand = false
				if game.Players.LocalPlayer.Character.Humanoid.RootPart and game.Players.LocalPlayer.Character.Humanoid.RootPart:FindFirstChild("NoGravity") then
					game.Players.LocalPlayer.Character.Humanoid.RootPart.NoGravity:Destroy()
				end
			end
		end
	end)
end
coroutine.wrap(YDVJD_fake_script)()
local function ZDIJQGN_fake_script() -- TextButton_2.LocalScript 
	local script = Instance.new('LocalScript', TextButton_2)

	local keys = {
		LeftShift = false,
	}
	game.Players.LocalPlayer:GetMouse().KeyDown:Connect(function(key)
		if key == "0" then
			keys.LeftShift = true
		end
	end)
	game.Players.LocalPlayer:GetMouse().KeyUp:Connect(function(key)
		if key == "0" then
			keys.LeftShift = false
		end
	end)
	local hack = false
	script.Parent.MouseButton1Click:Connect(function()
		hack = not hack
		if hack then
			script.Parent.Text = "Noclip: on"
			script.Parent.BackgroundColor3 = Color3.fromRGB(0,255,0)
			local humanoid = nil
			while hack do
				local fps = game["Run Service"].RenderStepped:Wait()
				if not humanoid or not humanoid.Parent or not humanoid.Parent.Parent then
					if game.Players.LocalPlayer.Character then
						if game.Players.LocalPlayer.Character:FindFirstChild("Humanoid") then
							humanoid = game.Players.LocalPlayer.Character.Humanoid
							if humanoid.RootPart then
								humanoid.RootPart.Anchored = true
							end
						end
					end
				else
					if humanoid.RootPart then
						humanoid.RootPart.Anchored = true
						local vel = humanoid.MoveDirection*humanoid.WalkSpeed
						if humanoid.Jump then
							vel = vel+Vector3.new(0,humanoid.WalkSpeed,0)
						end
						if keys.LeftShift then
							vel = vel+Vector3.new(0,-humanoid.WalkSpeed,0)
						end
						humanoid.RootPart.Velocity = ((humanoid.RootPart.Velocity-vel)*(0.75^(30/(1/fps))))+vel
						humanoid.RootPart.CFrame = (humanoid.RootPart.CFrame+(humanoid.RootPart.Velocity*fps))*CFrame.Angles(humanoid.RootPart.RotVelocity.X*fps,humanoid.RootPart.RotVelocity.Y*fps,humanoid.RootPart.RotVelocity.Z*fps)
					end
				end
			end
		else
			script.Parent.Text = "Noclip: off"
			script.Parent.BackgroundColor3 = Color3.fromRGB(255,0,0)
			game["Run Service"].RenderStepped:Wait()
			game["Run Service"].RenderStepped:Wait()
			if game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:FindFirstChild("Humanoid") and game.Players.LocalPlayer.Character.Humanoid.RootPart then
				game.Players.LocalPlayer.Character.Humanoid.RootPart.Anchored = false
			end
		end
	end)
end
coroutine.wrap(ZDIJQGN_fake_script)()
local function HJNV_fake_script() -- TextButton_3.LocalScript 
	local script = Instance.new('LocalScript', TextButton_3)

	-- Get the TextButton
	local button = script.parent
	
	-- Function to enable god mode
	local function enableGodMode(player)
		-- Get the character and set invincibility
		local character = player.Character or player.CharacterAdded:Wait()
		local humanoid = character:WaitForChild("Humanoid")
		humanoid.MaxHealth = math.huge
		humanoid.Health = math.huge
	end
	
	-- Function to disable god mode
	local function disableGodMode(player)
		-- Get the character and disable invincibility
		local character = player.Character or player.CharacterAdded:Wait()
		local humanoid = character:WaitForChild("Humanoid")
		humanoid.MaxHealth = 100
		humanoid.Health = 100
	end
	
	-- Function to handle button click
	local function onButtonClicked()
		-- Get the local player
		local player = game.Players.LocalPlayer
	
		-- Check if the player is already in god mode
		local character = player.Character
		if character then
			local humanoid = character:FindFirstChild("Humanoid")
			if humanoid and humanoid.MaxHealth == math.huge then
				-- Disable god mode
				disableGodMode(player)
				return
			end
		end
	
		-- Enable god mode
		enableGodMode(player)
	end
	
	-- Bind the button click event
	button.MouseButton1Click:Connect(onButtonClicked)
	
end
coroutine.wrap(HJNV_fake_script)()
local function TELAFFT_fake_script() -- TextButton_4.LocalScript 
	local script = Instance.new('LocalScript', TextButton_4)

	-- Get the TextButton
	local button = script.Parent
	
	-- Store the default walk speed
	local defaultWalkSpeed = 16
	
	-- Function to set the player's walk speed
	local function setWalkSpeed(player, speed)
		-- Get the character and humanoid
		local character = player.Character
		local humanoid = character and character:FindFirstChildOfClass("Humanoid")
	
		-- Check if the character and humanoid exist
		if humanoid then
			-- Set the walk speed
			humanoid.WalkSpeed = speed
		end
	end
	
	-- Function to handle button click
	local function onButtonClicked()
		-- Get the local player
		local player = game.Players.LocalPlayer
	
		-- Check if the player's current walk speed is the default speed
		if player.Character then
			local humanoid = player.Character:FindFirstChildOfClass("Humanoid")
			if humanoid and humanoid.WalkSpeed == defaultWalkSpeed then
				-- Set the walk speed to 50
				setWalkSpeed(player, 50)
			else
				-- Set the walk speed to the default value
				setWalkSpeed(player, defaultWalkSpeed)
			end
		end
	end
	
	-- Bind the button click event
	button.MouseButton1Click:Connect(onButtonClicked)
	
end
coroutine.wrap(TELAFFT_fake_script)()
local function IZXNREV_fake_script() -- TextButton_5.LocalScript 
	local script = Instance.new('LocalScript', TextButton_5)

	-- Get the TextButton
	local button = script.Parent
	
	-- Function to kill the character
	local function killCharacter(player)
		-- Check if the player has a character
		local character = player.Character
		if character then
			-- Kill the character
			local humanoid = character:FindFirstChild("Humanoid")
			if humanoid then
				humanoid.Health = 0
			end
		end
	end
	
	-- Function to handle button click
	local function onButtonClicked()
		-- Get the local player
		local player = game.Players.LocalPlayer
	
		-- Kill the player's character
		killCharacter(player)
	end
	
	-- Bind the button click event
	button.MouseButton1Click:Connect(onButtonClicked)
	
end
coroutine.wrap(IZXNREV_fake_script)()
local function VKMWPI_fake_script() -- TextButton_6.LocalScript 
	local script = Instance.new('LocalScript', TextButton_6)

	local player = game.Players.LocalPlayer
	local character = player.Character or player.CharacterAdded:Wait()
	local humanoid = character:WaitForChild("Humanoid")
	
	local jumpButton = script.Parent -- Assuming the GUI button is a child of the script
	
	local defaultJumpPower = humanoid.JumpPower -- Store the default jump power
	local increasedJumpPower = 200 -- Set the desired increased jump power
	
	local function onJumpButtonClicked()
		if humanoid.JumpPower == defaultJumpPower then
			humanoid.JumpPower = increasedJumpPower -- Set jump power to increased value
		else
			humanoid.JumpPower = defaultJumpPower -- Set jump power back to default value
		end
	end
	
	jumpButton.Activated:Connect(onJumpButtonClicked)
	
	
	
	
	
end
coroutine.wrap(VKMWPI_fake_script)()
local function DJWF_fake_script() -- TextButton_7.LocalScript 
	local script = Instance.new('LocalScript', TextButton_7)

	local player = game.Players.LocalPlayer
	local character = player.Character or player.CharacterAdded:Wait()
	
	local button = script.Parent -- Assuming the button is a child of the script
	
	local humanoid = character:WaitForChild("Humanoid")
	local originalTransparency = character:GetAttribute("OriginalTransparency") -- Store the original transparency value
	
	local function onTouchPart(part)
		local humanoid = part.Parent:FindFirstChildWhichIsA("Humanoid")
		if humanoid and humanoid == character:FindFirstChildOfClass("Humanoid") then
			local currentTransparency = humanoid.RootPart.Transparency
			if currentTransparency == 0 then
				humanoid.RootPart.Transparency = 1 -- Set transparency to 1 (invisible)
			else
				humanoid.RootPart.Transparency = 0 -- Set transparency back to 0 (visible)
			end
		end
	end
	
	button.Touched:Connect(onTouchPart)
	
end
coroutine.wrap(DJWF_fake_script)()
local function WVIMCWQ_fake_script() -- TextButton_8.LocalScript 
	local script = Instance.new('LocalScript', TextButton_8)

	local player = game.Players.LocalPlayer
	local character = player.Character or player.CharacterAdded:Wait()
	local humanoid = character:WaitForChild("Humanoid")
	
	local textButton = script.Parent -- Assuming the TextButton is a child of the script
	
	local function getClosestPlayer()
		local closestPlayer
		local closestDistance = math.huge
	
		for _, otherPlayer in ipairs(game.Players:GetPlayers()) do
			if otherPlayer ~= player and otherPlayer.Character then
				local otherCharacter = otherPlayer.Character
				local distance = (otherCharacter.PrimaryPart.Position - character.PrimaryPart.Position).magnitude
				if distance < closestDistance then
					closestDistance = distance
					closestPlayer = otherPlayer
				end
			end
		end
	
		return closestPlayer
	end
	
	local function teleportBehindPlayer()
		local closestPlayer = getClosestPlayer()
	
		if closestPlayer then
			local otherCharacter = closestPlayer.Character
			local otherHumanoid = otherCharacter:FindFirstChildOfClass("Humanoid")
			local otherRootPart = otherCharacter:FindFirstChild("HumanoidRootPart")
	
			if otherHumanoid and otherRootPart then
				local direction = (otherRootPart.Position - character.PrimaryPart.Position).unit
				local teleportPosition = otherRootPart.Position - (direction * 5) -- Teleport behind the player
	
				character:SetPrimaryPartCFrame(CFrame.new(teleportPosition))
			end
		end
	end
	
	local function onTouch()
		teleportBehindPlayer()
	end
	
	textButton.MouseButton1Click:Connect(onTouch)
	
end
coroutine.wrap(WVIMCWQ_fake_script)()
local function OVUZU_fake_script() -- TextButton_9.LocalScript 
	local script = Instance.new('LocalScript', TextButton_9)

	local frame = script.Parent
	local textButton = frame.Parent.TextButton
	
	
	local function toggleFrameVisibility()
		frame.Visible = not frame.Visible
	end
	
	
	textButton.Activated:Connect(toggleFrameVisibility)
	
end
coroutine.wrap(OVUZU_fake_script)()
local function GIGE_fake_script() -- ScreenGui.LocalScript 
	local script = Instance.new('LocalScript', ScreenGui)

	task.spawn(function()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/Pstrw/WhispHub/main/ACB1.lua"))();
		wait(0.4)
		loadstring(game:HttpGet("https://raw.githubusercontent.com/Pstrw/WhispHub/main/ACB2.lua"))();
	end)
	
end
coroutine.wrap(GIGE_fake_script)()
